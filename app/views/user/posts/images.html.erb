<div class="container">
  <div class="row">
    <div class="col-12 mt-3">
      <h4><%= @post.restaurant_name %>の画像</h4>
    </div>
  </div>
  <div class="row justify-content-center">
    <div>
      <% @post.images.each do |image| %>
        <%= image_tag image, size: "250x200" %>
      <% end %>
    </div>
  </div>
</div>